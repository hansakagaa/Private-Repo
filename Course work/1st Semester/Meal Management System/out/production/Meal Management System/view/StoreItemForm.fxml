<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="1000.0" style="-fx-background-color: #eeeeee;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StoreItemFormController">
   <children>
      <AnchorPane layoutX="160.0" layoutY="30.0" prefHeight="72.0" prefWidth="680.0" style="-fx-background-color: #ffffff; -fx-background-radius: 50;">
         <children>
            <Label layoutX="265.0" layoutY="21.0" text="Store Item">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="SupplierParent" layoutX="20.0" layoutY="126.0" prefHeight="170.0" prefWidth="960.0" style="-fx-background-color: #ffffff; -fx-background-radius: 25;">
         <children>
            <Label layoutX="20.0" layoutY="30.0" text="Supply Id : " />
            <Label layoutX="20.0" layoutY="75.0" text="Name : " />
            <JFXTextField fx:id="txtSupName" focusColor="#3498db" layoutX="100.0" layoutY="70.0" onKeyReleased="#supKeyReleasedOnAction" prefHeight="26.0" prefWidth="400.0" style="-fx-background-color: #eeeeee;" />
            <Label layoutX="20.0" layoutY="120.0" text="Address : " />
            <JFXTextField fx:id="txtSupAddress" focusColor="#3498db" layoutX="100.0" layoutY="115.0" onKeyReleased="#supKeyReleasedOnAction" prefHeight="26.0" prefWidth="200.0" style="-fx-background-color: #eeeeee;" />
            <JFXButton fx:id="btnEdit" layoutX="744.0" layoutY="17.0" onAction="#editedSaveOnAction" prefHeight="26.0" prefWidth="112.0" style="-fx-background-color: #74b9ff;" text="Edit &amp; Save" textFill="WHITE" underline="true">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </JFXButton>
            <ComboBox fx:id="cmbSupId" layoutX="100.0" layoutY="25.0" prefHeight="26.0" prefWidth="200.0" />
            <Label layoutX="480.0" layoutY="120.0" text="Contact : " />
            <JFXTextField fx:id="txtSupContact" focusColor="#3498db" layoutX="570.0" layoutY="115.0" onKeyReleased="#supKeyReleasedOnAction" prefHeight="26.0" prefWidth="200.0" style="-fx-background-color: #eeeeee;" />
            <Separator layoutX="396.0" layoutY="116.0" orientation="VERTICAL" prefHeight="40.0" prefWidth="6.0" />
            <Button fx:id="btnSave" layoutX="334.0" layoutY="25.0" mnemonicParsing="false" onAction="#supNewOnAction" style="-fx-background-color: #2be0f8;" text="New" underline="true">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Button>
            <JFXTextField fx:id="txtSupId" editable="false" focusColor="WHITE" layoutX="100.0" layoutY="25.0" prefHeight="26.0" prefWidth="200.0" unFocusColor="WHITE" />
            <Separator layoutX="315.0" layoutY="23.0" orientation="VERTICAL" prefHeight="30.0" prefWidth="6.0" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="ItemParent" layoutX="20.0" layoutY="312.0" prefHeight="220.0" prefWidth="960.0" style="-fx-background-color: #ffffff; -fx-background-radius: 25;">
         <children>
            <Label layoutX="25.0" layoutY="30.0" text="Item Code : " />
            <Label layoutX="25.0" layoutY="75.0" text="Item Description : " />
            <JFXTextField fx:id="txtItemDescription" editable="false" focusColor="#3498db" layoutX="145.0" layoutY="70.0" prefHeight="26.0" prefWidth="438.0" style="-fx-background-color: #eeeeee;" unFocusColor="WHITE" />
            <Label layoutX="25.0" layoutY="120.0" text="Unit Price : " />
            <JFXTextField fx:id="txtItemUnitPrice" focusColor="#3498db" layoutX="145.0" layoutY="115.0" onKeyReleased="#itemKeyReleasedOnAction" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #eeeeee;" text="0.00" />
            <JFXButton fx:id="btnAdd" layoutX="761.0" layoutY="153.0" onAction="#addStoreOnAction" prefHeight="40.0" prefWidth="130.0" style="-fx-background-color: #6c5ce7;" text="Add Store" textFill="#eeebeb" underline="true">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </JFXButton>
            <Label layoutX="444.0" layoutY="165.0" text="Qty : " />
            <JFXTextField fx:id="txtItemQty" focusColor="#3498db" layoutX="503.0" layoutY="160.0" onKeyReleased="#itemKeyReleasedOnAction" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #eeeeee;" />
            <Separator layoutX="374.0" layoutY="110.0" orientation="VERTICAL" prefHeight="80.0" prefWidth="6.0" />
            <ComboBox fx:id="cmbItemCode" layoutX="145.0" layoutY="25.0" prefHeight="26.0" prefWidth="200.0" />
            <Label layoutX="25.0" layoutY="165.0" text="Total Price : " />
            <JFXTextField fx:id="txtTotalPrice" alignment="BOTTOM_RIGHT" editable="false" focusColor="#3498db" layoutX="145.0" layoutY="160.0" prefHeight="26.0" prefWidth="160.0" style="-fx-background-color: #eeeeee;" text="0.00" unFocusColor="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></JFXTextField>
            <Label layoutX="309.0" layoutY="163.0" text="/=">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
            <Label layoutX="540.0" layoutY="146.0" text="Max value '9999'">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Label fx:id="lblDate" layoutX="730.0" layoutY="18.0" text="0000:00:00">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
